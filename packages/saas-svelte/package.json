{
	"name": "@saas-ui/svelte",
	"version": "0.0.1",
	"description": "A Svelte implementation of the open-source SaaS Design System",
	"license": "MIT",
	"type": "module",
	"svelte": "./index.ts",
	"main": "./index.ts",
	"exports": {
		".": {
			"types": "./index.ts",
			"svelte": "./index.ts",
			"default": "./index.ts"
		},
		"./components/*": {
			"types": "./components/*/index.ts",
			"svelte": "./components/*/index.ts",
			"default": "./components/*/index.ts"
		},
		"./layout/*": {
			"types": "./layout/*/index.ts",
			"svelte": "./layout/*/index.ts",
			"default": "./layout/*/index.ts"
		},
		"./typography/*": {
			"types": "./typography/*/index.ts",
			"svelte": "./typography/*/index.ts",
			"default": "./typography/*/index.ts"
		},
		"./utilities/*": {
			"types": "./utilities/*/index.ts",
			"svelte": "./utilities/*/index.ts",
			"default": "./utilities/*/index.ts"
		},
		"./utils/*": {
			"types": "./utils/*",
			"default": "./utils/*"
		},
		"./styles/*": {
			"default": "./styles/*"
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/saas-js/saas-ui-svelte.git",
		"directory": "packages/saas-svelte"
	},
	"bugs": {
		"url": "https://github.com/saas-js/saas-ui-svelte/issues"
	},
	"homepage": "https://saas-js.github.io/saas-ui-svelte",
	"keywords": ["svelte", "components", "ui", "saas", "design-system", "tailwindcss"],
	"peerDependencies": {
		"@saas-ui/tailwind-preset": "^0.0.2-next.0",
		"svelte": "^5.0.0"
	},
	"peerDependenciesMeta": {
		"@saas-ui/tailwind-preset": {
			"optional": true
		}
	},
	"dependencies": {
		"@ark-ui/svelte": "^5.15.0",
		"@tanstack/svelte-virtual": "^3.13.18",
		"clsx": "^2.1.1",
		"phosphor-svelte": "^3.1.0",
		"runed": "^0.37.1",
		"sveltekit-superforms": "^2.29.1",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.2.2",
		"zod": "^4.3.6"
	}
}
